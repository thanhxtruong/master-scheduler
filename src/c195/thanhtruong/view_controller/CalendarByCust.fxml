<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="1000.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195.thanhtruong.view_controller.CalendarByCustController">
   <children>
      <TabPane layoutX="-5.0" layoutY="-1.0" prefHeight="1000.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <tabs>
          <Tab closable="false" text="Month">
              <content>
                    <fx:include fx:id="calByMonth" source="CalendarByMonth.fxml" />
                </content>
            </Tab>    
          <Tab fx:id="weekTab" onSelectionChanged="#displayApptByWeek" text="Week">
                <content>
                    <fx:include fx:id="calByWeek" source="CalendarByWeek.fxml" />
                </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
